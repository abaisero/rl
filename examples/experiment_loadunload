#!/usr/bin/zsh

opts=(--runs 10 --episodes 10 --steps 10)
# opts=(--runs 100 --episodes 20000 --steps 100)
pomdp=loadunload

for N in `seq 3 7`; do
  ./learning.py $opts --out ${pomdp}_isgpomdp_fsc_$N.npy $pomdp.pomdp isgpomdp fsc $N
done

for N in `seq 5 7`; do
for K in `seq 3 $((N-2))`; do
  ./learning.py $opts --out ${pomdp}_isgpomdp_fsc_${N}_${K}.npy $pomdp.pomdp isgpomdp fsc_sparse $N $K
done
done

for N in `seq 3 7`; do
  ./learning.py $opts --out ${pomdp}_expgpomdp_fsc_$N.npy $pomdp.pomdp expgpomdp fsc --belief $N
done

# TODO belief-sparse-FSC model does not have nmodel
